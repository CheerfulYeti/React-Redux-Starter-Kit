{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "react-require",
    "transform-class-properties",
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    ["transform-regenerator", {
      "async": false
    }],
    ["transform-runtime", {
      "helpers": true,
      "polyfill": true,
      "regenerator": true
    }]
  ]
}
